{
  "name": "ollie-ui",
  "version": "1.0.3",
  "description": "A Mobile UI Component Library for Vue.js.",
  "main": "lib/ollie-ui.common.js",
  "files": [
    "lib",
    "packages"
  ],
  "scripts": {
    "bootstrap": "yarn || npm i",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "clean": "rimraf lib && rimraf packages/*/lib && rimraf test/**/coverage",
    "build:style": "node build/bin/gen-cssfile && gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk",
    "build:com": "node build/bin/build-components-json.js",
    "build:entry": "node build/bin/build-entry.js",
    "build:lib-umd": "webpack --config build/webpack.umd.js",
    "build:lib-common": "webpack --config build/webpack.common.js",
    "build:lib-component": "webpack --config build/webpack.component.js",
    "build:lib": "npm run clean && npm run build:style && npm run build:com && npm run build:entry && npm run lint && npm run build:lib-umd && npm run build:lib-common && npm run build:lib-component"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zc95/ollie-ui.git",
    "directory": "packages"
  },
  "homepage": "https://github.com/zc95/ollie-ui",
  "keywords": [
    "ollie-ui",
    "ollieui",
    "ollie ui",
    "Ollie UI",
    "ui",
    "vue",
    "vue2.6",
    "mobile",
    "frontend",
    "component",
    "components",
    "library"
  ],
  "license": "MIT",
  "bugs": "https://github.com/zc95/ollie-ui/issues",
  "author": "Zhang Cheng <571346800@qq.com>",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.7.1",
    "@vue/cli-plugin-babel": "~4.5.17",
    "@vue/cli-plugin-eslint": "~4.5.17",
    "@vue/cli-plugin-router": "~4.5.17",
    "@vue/cli-plugin-unit-jest": "~4.5.17",
    "@vue/cli-service": "~4.5.17",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^7.1.5",
    "chalk": "^4.1.1",
    "core-js": "^3.6.5",
    "css-loader": "^2.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "json-templater": "^1.2.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "rimraf": "^3.0.2",
    "style-loader": "^0.23.1",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.0.1",
    "vue": "^2.6.11",
    "vue-loader": "^15.7.2",
    "vue-router": "^3.2.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.7.2",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-less": "^4.0.1",
    "cp-cli": "^1.0.2"
  },
  "peerDependencies": {
    "vue": "^2.5.1"
  }
}
